{
  "extends": "@atomix/eslint-config",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "consistent-return": "off",
    "guard-for-in":"off",
    "no-cond-assign":"off",
    "linebreak-style":"off",
    "prefer-promise-reject-errors": "off",
    "no-nested-ternary": "off",
    "prefer-numeric-literals": "off",
    "id-match": "off"
  }
}